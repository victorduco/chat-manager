{
  "dockerfile_lines": [
    "COPY langgraph-app /deps/langgraph-app",
    "COPY libs/conversation_states /deps/conversation_states"
  ],
  "graphs": {
    "graph_router": "lg_main.g_command_router.graph:graph_router",
    "graph_supervisor": "lg_main.g_supervisor.graph:graph_supervisor",
    "graph_dispatcher": "lg_main.g_dispatcher.graph:graph_dispatcher",
    "graph_chat_manager": "lg_main.g_chat_manager.graph:graph_chat_manager",
    "graph_daily_runner": "lg_main.g_daily_runner.graph:graph_daily_runner",
    "graph_daily_meta_improver": "lg_main.g_daily_meta_improver.graph:graph_daily_meta_improver"
  },
  "env": "./.env",
  "python_version": "3.13",
  "dependencies": [
    "langchain>=1.2.9",
    "langchain-community>=0.3.21",
    "langchain-core>=1.2.9",
    "langchain-openai>=1.1.8",
    "langchain-text-splitters>=1.1.0",
    "langgraph>=1.0.8",
    "langgraph-checkpoint-sqlite>=2.0.6",
    "langgraph-cli>=0.4.12",
    "langgraph-prebuilt>=0.1.8",
    "langgraph-runtime-inmem>=0.0.11",
    "langgraph-sdk>=0.3.4",
    "langsmith>=0.6.9",
    "tavily-python>=0.5.4",
    "trustcall>=0.0.36",
    "wikipedia>=1.4.0"
  ],
  "server": {
    "port": 2024,
    "host": "0.0.0.0"
  },
  "studio": {
    "title": "Victor AI",
    "description": "Victor AI is a large language model (LLM) that can be used for various natural language processing tasks. It is designed to be a versatile and powerful tool for developers and researchers."
  }
}
